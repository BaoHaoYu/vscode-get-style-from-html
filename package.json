{
  "name": "get-style-from-html",
  "displayName": "Get style from html",
  "description": "Get styles from html, now only need to write css class once in html",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.get.style.from.html"
  ],
  "main": "./out/extension.js",
  "resource": "src",
  "icon": "images/icon.png",
  "publisher": "BaoHaoYu",
  "license": "LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/BaoHaoYu/vscode-get-style-from-html.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.get.style.from.html",
        "title": "Get style from html"
      }
    ],
    "configuration": {
      "title": "Get style from html",
      "properties": {
        "get.style.from.html.cssStyle": {
          "type": "string",
          "deprecationMessage": "sass | css"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/copy-paste": "^1.1.30",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.7.4",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "lodash": "^4.17.15"
  }
}
